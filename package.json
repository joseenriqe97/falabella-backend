{
  "name": "falabella-api",
  "version": "1.0.0",
  "description": "FALABELLA API",
  "main": "index.js",
  "scripts": {
    "start": "babel-node -r dotenv/config src/index.js",
    "start:watch": "nodemon --exec babel-node -r dotenv/config src/index.js",
    "start:prod": "node -r dotenv/config dist/index.js",
    "build": "rimraf dist/ && babel src --out-dir dist",
    "test": "mocha --exit --require @babel/register -r dotenv/config --recursive --timeout 2000 'src/test/unitest/**/**spec.js'",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "release": "standard-version",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joseenriqe97/falabella.git"
  },
  "author": "Jose Bonilla",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joseenriqe97/falabella.git/issues"
  },
  "homepage": "https://github.com/joseenriqe97/falabella#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.14.0",
    "express-mung": "^0.5.1",
    "express-routes-versioning": "^1.0.0",
    "helmet": "^4.6.0",
    "http-errors": "^1.8.0",
    "http-status": "^1.0.1",
    "log4js": "^6.3.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/register": "^7.13.16",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "commitizen": "^4.2.4",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "mocha": "^8.4.0",
    "nock": "^13.1.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.1",
    "standard-version": "^9.3.0",
    "supertest": "^6.1.3",
    "tslint": "^6.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
